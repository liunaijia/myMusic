    "deploy:api:deploy-stack": "sam deploy --template-file ./.aws-sam/template.yaml --stack-name myMusic --capabilities CAPABILITY_IAM",
    "lint:css": "stylelint ./src/**/*.jsx",
    "lint:eslint": "eslint --ext .js --ext .jsx src",
    "test": "jest",
    "deploy:web:build": "webpack --config webpack-web.config.js --mode production",
    "dev:web": "webpack-dev-server --config webpack-web.config.js --mode development --hot",
    "storybook": "start-storybook"
